{
  "manifest_version": 2,

  "name": "Boston Globe Ad Remover",
  "description": "Hides ads on bostonglobe.com",
  "version": "1.0",

  "content_scripts": [
    {
      "matches": ["http://www.bostonglobe.com/*", "http://www.betaboston.com/*"],
      "js": ["jquery.min.js", "ad_remover.js"]
    }
  ],

  "page_action": {
    "default_icon": "icon.png"
  }
}
